
WHITESPACE = _{ " " | "\t" }
key = { (ASCII_ALPHANUMERIC | "(" | ")" | "_" | " " )+ }
ass = { ":" }
value = { (ASCII_ALPHANUMERIC | WHITESPACE | "(" | ")" | "_" | " " | "@" | "." | "," | "-")* }

// A single entry, matches "key : value\n"
row = _{ key ~ WHITESPACE* ~ ass ~ WHITESPACE* ~ value ~ NEWLINE{1,2} }

section = { row+ }

file = _{ row+ ~ EOI }
